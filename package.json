{
  "name": "arena-core",
  "version": "1.0.0",
  "description": "Core browser client for ARENA",
  "author": "CONIX Center",
  "license": "BSD-3-Clause",
  "default": "dist/arena-core.js",
  "browserslist": [
    "last 3 versions"
  ],
  "transform": [
    [
      "babelify",
      {
        "comments": false
      }
    ]
  ],
  "targets": {
    "default": {
      "publicUrl": "./dist",
      "engines": {
        "browsers": "last 3 versions"
      }
    }
  },
  "dependencies": {
    "aframe": "^1.2.0",
    "aframe-extras": "git+https://github.com/n5ro/aframe-extras.git#351608a",
    "aframe-particle-system-component": "^1.1.3",
    "aframe-spe-particles-component": "^1.0.4",
    "aframe-thickline-component": "^0.0.3",
    "chat": "^1.0.8",
    "comlink": "^4.3.0",
    "core-js": "^3.11.1",
    "he": "^1.2.0",
    "jquery": "^3.6.0",
    "linkifyjs": "^2.1.9",
    "paho-mqtt": "^1.1.0",
    "sweetalert2": "^10.16.6",
    "three-pathfinding": "^0.14.1",
    "webxr-polyfill": "^2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@parcel/babel-plugin-transform-runtime": "^2.0.0-beta.2",
    "@parcel/babel-preset-env": "^2.0.0-beta.2",
    "babelify": "^10.0.0",
    "eslint": "^7.25.0",
    "eslint-config-google": "^0.14.0",
    "jsdoc": "^3.6.6",
    "jsdoc-to-markdown": "^7.0.1",
    "parcel": "^2.0.0-beta.2"
  },
  "scripts": {
    "collect-static": "cp src/apriltag/apriltag_wasm.* dist/",
    "prewatch": "npm run collect-static",
    "watch": "parcel watch src/index.js",
    "prebuild": "npm run collect-static",
    "build": "parcel build src/index.js --public-url ./dist",
    "build-nominify": "parcel build src/index.js --public-url ./dist --no-optimize",
    "prebuild-main": "npm run collect-static",
    "build-main": "parcel build src/index.js --public-url ./dist",
    "predocs": "echo '# A-Frame components (modules) added to support ARENA core functionality\n' > src/components/README.md",
    "docs": "jsdoc2md src/components/*.js >> src/components/README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conix-center/ARENA-core.git"
  },
  "bugs": {
    "url": "https://github.com/conix-center/ARENA-core/issues"
  },
  "homepage": "https://github.com/conix-center/ARENA-core#readme"
}
